{
  "version": 2,
  "name": "sandbox",
  "builds": [
    { "src": "static/index.html", "use": "@now/html-minifier" },
    { "src": "static/styles.css", "use": "@now/html-minifier" }
  ],
  "routes": [
    { "src": "/", "status": 301, "headers": { "Location": "/static/index.html" } },
    { "src": "/static", "dest": "/static/index.html" }
  ]
}
